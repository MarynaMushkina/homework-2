<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UdemyWeb">
    <test name="SignUp test">
        <classes>
            <class name="com.udemy.SignUpTest">
                <methods>
                    <include name="signUpWithValidCredentials"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="SignIn test">
        <classes>
            <class name="com.udemy.SignInUsingCookieTest">
                <methods>
                    <include name="signInUsingCookie"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Search test">
        <classes>
            <class name="com.udemy.SearchTest">
                <methods>
                    <include name="testSearchField"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Categories test">
        <classes>
            <class name="com.udemy.CategoriesTest">
                <methods>
                    <include name="TestMenuList"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>