<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UdemyWeb">
    <test name="SignUp tests">
        <classes>
            <class name="com.udemy.SignUpTest">
                <methods>
                    <include name="signUpWithValidCredentials"/>
                </methods>
            </class>
            <class name="com.udemy.SignInTest">
                <methods>
                    <include name="signIn"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>